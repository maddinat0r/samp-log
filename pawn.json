{
    "user": "maddinat0r",
    "repo": "samp-log",
    "resources": [
        {
            "name": "^log-plugin-(.*)-[Ll]inux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": "pawno/include",
            "plugins": ["plugins/log-plugin.so"],
            "files": {
                "log-core.so": "log-core.so"
            }
        },
        {
            "name": "^log-plugin-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": "pawno/include",
            "plugins": ["plugins/log-plugin.dll"],
            "files": {
                "log-core.dll": "log-core.dll"
            }
        }
    ],
    "contributors": ["maddinat0r"],
    "runtime": {
        "plugins": ["maddinat0r/log-plugin"]
    }
}